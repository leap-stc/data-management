    "Link": {
      "title": "Link",
      "type": "object",
      "properties": {
        "label": {
          "title": "Label",
          "description": "Label of the link",
          "type": "string"
        },
        "href": {
          "title": "Href",
          "description": "URL of the link",
          "minLength": 1,
          "maxLength": 65536,
          "format": "uri",
          "type": "string"
        }
      },
      "required": ["label", "href"]
    }
